{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Angulogin",
  "version": "0.1.0",
  "identifier": "com.johntran.angulogin",
  "build": {
    "frontendDist": "../dist/angulogin/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "npm run start",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Angulogin",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": {
      "extensions/stealth/*": "extensions/stealth/"
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}