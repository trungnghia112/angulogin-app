    {
        "id": "tiktok-search-like",
        "version": "2.0.0",
        "metadata": {
            "title": "TikTok Search & Comment Like",
            "description": "Search videos, browse & random like comments to boost activity.",
            "platform": "TikTok",
            "platformIcon": "pi-video",
            "author": "AnguLogin Team",
            "tags": [
                "social",
                "engagement",
                "tiktok"
            ],
            "createdAt": "2026-01-15",
            "updatedAt": "2026-02-20",
            "isPremium": true
        },
        "stats": {
            "usageCount": 1262
        },
        "requirements": {
            "note": "Requires TikTok account login"
        },
        "overview": "Visit Home → Search Keywords → Browse Videos → Like Comments → Loop",
        "steps": [
            {
                "order": 1,
                "action": "navigate",
                "url": "https://www.tiktok.com",
                "description": "Open TikTok homepage",
                "waitForSelector": "[data-e2e='search-box-input'], input[type='search'], form[action*='search'] input",
                "timeout": 15000,
                "humanDelay": [
                    2000,
                    4000
                ]
            },
            {
                "order": 2,
                "action": "type",
                "selector": "[data-e2e='search-box-input']",
                "fallbackSelectors": [
                    "input[type='search']",
                    "input[name='q']",
                    "input[placeholder*='Search']"
                ],
                "value": "{{searchText}}",
                "description": "Type search keyword into search box and submit",
                "timeout": 8000,
                "humanDelay": [
                    1000,
                    3000
                ]
            },
            {
                "order": 3,
                "action": "wait",
                "waitMs": 4000,
                "description": "Wait for search results to load",
                "humanDelay": [
                    1000,
                    2000
                ]
            },
            {
                "order": 4,
                "action": "scroll",
                "description": "Scroll through search results to load videos",
                "iterations": 3,
                "humanDelay": [
                    2000,
                    4000
                ]
            },
            {
                "order": 5,
                "action": "click",
                "description": "Click on a video from search results",
                "jsExpression": "(() => { const videos = document.querySelectorAll('[data-e2e=\"search-card-desc\"] a, [class*=\"DivVideoCardContainer\"] a, a[href*=\"/video/\"]'); if (videos.length) { const idx = Math.floor(Math.random() * Math.min(videos.length, {{viewVideoCount}})); videos[idx]?.click(); return 'clicked video ' + idx; } return 'no videos found'; })()",
                "humanDelay": [
                    3000,
                    6000
                ]
            },
            {
                "order": 6,
                "action": "click",
                "description": "Like random comments based on probability",
                "jsExpression": "(() => { const likeRate = {{likeRate}}; const btns = document.querySelectorAll('[data-e2e=\"comment-like-icon\"], [class*=\"like-icon\"], svg[class*=\"Like\"]'); let liked = 0; btns.forEach((btn, i) => { if (i < {{CommentLikes}} && Math.random() * 10 < likeRate) { btn.click(); liked++; } }); return 'liked ' + liked + ' comments'; })()",
                "humanDelay": [
                    2000,
                    5000
                ]
            }
        ],
        "variables": [
            {
                "name": "searchText",
                "type": "string",
                "required": true,
                "description": "Keywords, one topic per line"
            },
            {
                "name": "viewVideoCount",
                "type": "number",
                "required": false,
                "default": 3,
                "description": "Videos to browse per search (Default: 3)"
            },
            {
                "name": "CommentLikes",
                "type": "number",
                "required": false,
                "default": 1,
                "description": "Comment likes per video (Default: 1)"
            },
            {
                "name": "likeRate",
                "type": "number",
                "required": false,
                "default": 7,
                "description": "Like probability 0-10. 10=100% (Default: 7)"
            }
        ]
    },
