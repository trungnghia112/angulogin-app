    {
        "id": "tiktok-search-like",
        "version": "1.0.0",
        "metadata": {
            "title": "TikTok Search & Comment Like",
            "description": "Search videos, browse & random like comments to boost activity.",
            "platform": "TikTok",
            "platformIcon": "pi-video",
            "author": "AnguLogin Team",
            "tags": [
                "social",
                "engagement",
                "tiktok"
            ],
            "createdAt": "2026-01-15",
            "updatedAt": "2026-02-19",
            "isPremium": true
        },
        "stats": {
            "usageCount": 1262
        },
        "requirements": {
            "note": "Requires TikTok account login"
        },
        "overview": "Visit Home → Search Keywords → Browse Videos → Like Comments by Probability → Screenshot → Loop",
        "steps": [
            {
                "order": 1,
                "action": "navigate",
                "url": "https://tiktok.com",
                "description": "Visit TikTok homepage, read the search keywords list"
            },
            {
                "order": 2,
                "action": "type",
                "selector": "input[type=search]",
                "value": "{{searchText}}",
                "description": "Iterate keywords, input search term to search"
            },
            {
                "order": 3,
                "action": "scroll",
                "description": "Get video list, randomly enter video detail page by set count"
            },
            {
                "order": 4,
                "action": "click",
                "description": "Open comment section, calculate whether to like based on probability"
            },
            {
                "order": 5,
                "action": "loop",
                "description": "Loop like comments by set count, screenshot to save record"
            },
            {
                "order": 6,
                "action": "extract",
                "description": "Save like data, switch to the next video or keyword to continue"
            }
        ],
        "variables": [
            {
                "name": "searchText",
                "type": "string",
                "required": true,
                "description": "Keywords, one topic per line"
            },
            {
                "name": "viewVideoCount",
                "type": "number",
                "required": false,
                "default": 3,
                "description": "Videos to browse per search (Default: 3)"
            },
            {
                "name": "CommentLikes",
                "type": "number",
                "required": false,
                "default": 1,
                "description": "Comment likes per video (Default: 1)"
            },
            {
                "name": "likeRate",
                "type": "number",
                "required": false,
                "default": 7,
                "description": "Like probability 0-10. 10=100% (Default: 7)"
            }
        ]
    },
