                "timeout": 8000,
                "humanDelay": [
                    1000,
                    2000
                ]
            },
            {
                "order": 3,
                "action": "click",
                "selector": "button.btn-solid-primary",
                "fallbackSelectors": [
                    "button[type='button'].shopee-searchbar-input__search-icon",
                    ".shopee-searchbar-input__search-icon",
                    "button[class*='search']"
                ],
                "description": "Click the Search button",
                "timeout": 5000,
                "humanDelay": [
                    2000,
                    4000
                ]
            },
            {
                "order": 4,
                "action": "wait",
                "waitMs": 4000,
                "description": "Wait for search results to load",
                "humanDelay": [
                    1000,
                    2000
                ]
            },
            {
                "order": 5,
                "action": "scroll",
                "description": "Scroll through product listings",
                "iterations": 4,
                "humanDelay": [
                    2000,
                    4000
                ]
            },
            {
                "order": 6,
                "action": "click",
                "description": "Click on a random product to view details",
                "jsExpression": "(() => { const items = document.querySelectorAll('.shopee-search-item-result__item a, [data-sqe=\"link\"] a, .col-xs-2-4 a[href*=\"-i.\"]'); const count = Math.min(items.length, {{browseCount}}); if (count > 0) { const idx = Math.floor(Math.random() * count); items[idx]?.click(); return 'clicked product ' + idx + ' of ' + items.length; } return 'no products found'; })()",
